<h1>All Events</h1>
	<%= form_tag '/admin/events/choose', :method => :put do %>
		<ul>
			<% for event in @events %>
				<li>
					<%= check_box_tag "event_ids[]", event.id, (event.choose==1) %>
					<%= event.event %>
				</li>
			<% end %>
		</ul>
		<%= submit_tag "Update with choose" %>
	<% end %>
